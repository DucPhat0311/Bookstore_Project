<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:QUAN_LY.ViewModel"
                    xmlns:view="clr-namespace:QUAN_LY.View">
    <DataTemplate DataType="{x:Type vm:HomeViewModel}">
        <view:HomeView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:BookViewModel}">
        <view:BookView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:InventoryViewModel}">
        <view:InventoryView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:OrderViewModel}">
        <view:OrderView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:RevenueStatisticsViewModel}">
        <view:RevenueStatisticsView/>
    </DataTemplate>



</ResourceDictionary>