<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:QUAN_LY.ViewModel"
                    xmlns:view="clr-namespace:QUAN_LY.View">
    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:Book_ManagementVM}">
        <view:Book_Management/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:InventoryVM}">
        <view:Inventory/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:OrderVM}">
        <view:Order/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:Revenue_StatisticsVM}">
        <view:Revenue_Statistics/>
    </DataTemplate>



</ResourceDictionary>